<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  	<servlet>
	  <servlet-name>login</servlet-name>
	  <servlet-class>com.ipartek.formacion.javaee.servlets.LoginServlet</servlet-class>
	</servlet>
 	<servlet>
  		<servlet-name>registro</servlet-name>
  		<servlet-class>com.ipartek.formacion.javaee.servlets.RegistroServlet</servlet-class>
	</servlet>
 	<servlet-mapping>
  		<servlet-name>/login</servlet-name>
  		<url-pattern>/login</url-pattern>
	</servlet-mapping>
 	<servlet-mapping>
  		<servlet-name>registro</servlet-name>
  		<url-pattern>/registro</url-pattern>
	</servlet-mapping>
</web-app>
